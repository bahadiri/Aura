{
    "name": "@minnai/aura",
    "version": "0.1.7",
    "description": "A UX for the Gen AI age based on UI Retrieval Philosophy.",
    "keywords": [
        "react",
        "generative-ai",
        "agentic-ui",
        "ui-retrieval",
        "aura"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/bahadiri/Aura.git"
    },
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        },
        "./sdk": {
            "types": "./dist/sdk/index.d.ts",
            "import": "./dist/sdk/index.js"
        },
        "./sdk/index": {
            "types": "./dist/sdk/index.d.ts",
            "import": "./dist/sdk/index.js"
        },
        "./atmosphere": {
            "types": "./dist/atmosphere/index.d.ts",
            "import": "./dist/atmosphere/index.js"
        },
        "./atmosphere/manifests": {
            "types": "./dist/atmosphere/manifests.d.ts",
            "import": "./dist/atmosphere/manifests.js"
        },
        "./atmosphere/tasks": {
            "types": "./dist/atmosphere/tasks/index.d.ts",
            "import": "./dist/atmosphere/tasks/index.js"
        },
        "./atmosphere/note-taker": {
            "types": "./dist/atmosphere/note-taker/index.d.ts",
            "import": "./dist/atmosphere/note-taker/index.js"
        },
        "./atmosphere/youtube-player": {
            "types": "./dist/atmosphere/youtube-player/index.d.ts",
            "import": "./dist/atmosphere/youtube-player/index.js"
        },
        "./controller/Space": {
            "types": "./dist/controller/Space.d.ts",
            "import": "./dist/controller/Space.js"
        },
        "./controller/useController": {
            "types": "./dist/controller/useController.d.ts",
            "import": "./dist/controller/useController.js"
        },
        "./flux/index": {
            "types": "./dist/flux/index.d.ts",
            "import": "./dist/flux/index.js"
        },
        "./hooks/useVoiceInput": {
            "types": "./dist/hooks/useVoiceInput.d.ts",
            "import": "./dist/hooks/useVoiceInput.js"
        },
        "./storage/config": {
            "types": "./dist/storage/config.d.ts",
            "import": "./dist/storage/config.js"
        }
    },
    "scripts": {
        "dev": "vite",
        "build": "tsc -p tsconfig.build.json && npm run copy-styles",
        "copy-styles": "npx copyfiles -u 1 \"src/**/*.css\" dist",
        "build:vite": "vite build",
        "preview": "vite preview",
        "test": "vitest run",
        "prepublishOnly": "npm run build"
    },
    "dependencies": {
        "idb-keyval": "^6.2.2",
        "react-rnd": "^10.4.1"
    },
    "peerDependencies": {
        "firebase": ">=10.0.0",
        "react": ">=18.0.0",
        "react-dom": ">=18.0.0"
    },
    "devDependencies": {
        "@playwright/test": "^1.57.0",
        "@testing-library/jest-dom": "^6.9.1",
        "@testing-library/react": "^16.3.1",
        "@types/react": "^18.2.66",
        "@types/react-dom": "^18.2.22",
        "@vitejs/plugin-react": "^4.2.1",
        "copyfiles": "^2.4.1",
        "jsdom": "^27.4.0",
        "typescript": "^5.2.2",
        "vite": "^5.2.0",
        "vitest": "^1.5.0"
    }
}
